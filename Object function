class pelajar:
    def __init__(self,name,major,gpa):

        self.name = name
        self.major = major
        self.gpa = gpa

    def on_honor(self):
        if self.gpa >= 3.5:
            return True
        else:
            return False
        


from pelajar import pelajar

student1 = pelajar("Haziq", "science", 3.6)
student2 = pelajar("Ali", "Math", 3.4)

print(student2.on_honor())


